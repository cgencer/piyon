<?php

/**
 * @file
 * Uikit Node Accordion Views module help and theme functions.
 */

// Store Uikit Node Accordion preprocess theme functions in a separate .inc file.
\Drupal::moduleHandler()->loadInclude('uikit_node_accordion', 'inc', 'uikit_node_accordion.theme');

/**
 * Implements hook_theme().
 */
function uikit_node_accordion_theme($existing, $type, $theme, $path) {
  return [
    'uikit_node_accordion' => [
      'variables' => ['view' => NULL, 'rows' => NULL],
      'file' => 'uikit_node_accordion.theme.inc',
      'path' => drupal_get_path('module', 'uikit_node_accordion') . '/templates',
    ],
  ];
}
