<?php

use Drupal\Core\Site\Settings;

/**
 * Implements hook_library_info_build().
 */
function watchdog_event_extras_library_info_build() {

  $api_key = Settings::get('google_maps_api_key');

  $libraries = [];
  $libraries['wee.map'] = [
    'js' => [
      '//maps.googleapis.com/maps/api/js?key='.$api_key => [],
    ]
  ];
  return $libraries;
}
